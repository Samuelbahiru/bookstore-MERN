<h2>Search Book</h2>
<form action="/books" method="GET">
    <div>
        <label>Title</label>
        <input text="text" name="title" value="<%= searchOptions.title %>"/>
    </div>
    <div>
        <label>Published After</label>
        <input text="text" name="publishedAfter" value="<%= searchOptions.publishedAfter %>"/>
    </div>
    <div>
        <label>Published Before</label>
        <input text="text" name="publishedBefore" value="<%= searchOptions.publishedBefore %>"/>
    </div>

<button type="submit">Search</button>
</form>
<br/>
<div>
    <% books.forEach((book)=>{%>
        <a href="/books/<%= book.id %>"><img height="150" width="100" src="<%= book.coverImagePath %>"/></a>
    
    <% })%>
</div>
