<h2 class="page-header"> <%= book.title %> </h2>
<img width="100" height= "150" src="<%= book.coverImagePath %>" />
<div>
Author: <%= book.author.name %>
</div>
<div>
publishDate: <%= book.publishDate.toDateString()%>
</div>
<div>
pageCount: <%= book.author.pageCount%>
</div>
<div>
Description: <%= book.author.description%>
</div>
<a href="/books/<%= book.id %>/edit">Edit</a>
<%- include("../partials/deleteForm", {url: `/books/${book.id}`})%>
<a href="/authors/<%= book.author.id %>">View Author</a>
